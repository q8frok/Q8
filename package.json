{
  "name": "q8",
  "version": "1.0.0",
  "private": true,
  "description": "Q8: The Omni-Model Personal Assistant - Local-First Multi-Agent AI Dashboard",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "storybook": "turbo storybook",
    "validate-schemas": "tsx scripts/validate-schema-sync.ts",
    "generate-migration": "tsx scripts/generate-migration.ts",
    "quality": "powershell -ExecutionPolicy Bypass -File scripts/quality-check.ps1",
    "quality:verbose": "powershell -ExecutionPolicy Bypass -File scripts/quality-check.ps1 -Verbose",
    "quality:fast": "powershell -ExecutionPolicy Bypass -File scripts/quality-check.ps1 -SkipTests -SkipBuild",
    "optimize:claude": "powershell -ExecutionPolicy Bypass -File scripts/optimize-claude.ps1",
    "supabase:safety": "node scripts/check-supabase-safety.mjs",
    "precommit": "pnpm typecheck && pnpm build --filter=@q8/web"
  },
  "devDependencies": {
    "@turbo/gen": "^2.0.0",
    "prettier": "^3.2.5",
    "supabase": "^2.72.7",
    "tsx": "^4.7.0",
    "turbo": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
